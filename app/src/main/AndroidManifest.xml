<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.yasania.appislami">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppIslami">
        <activity android:name=".activities.AboutMeActivity"></activity>
        <activity android:name=".activities.DoaActivity" />
        <activity android:name=".activities.AyatKursiActivity" />
        <activity android:name=".activities.NiatShalatActivity" />
        <activity android:name=".activities.BacaanShalatActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>